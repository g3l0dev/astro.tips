{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@components/": [
        "src/components/"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "react"
  }
}